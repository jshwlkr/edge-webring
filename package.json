  "license": "",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "concurrently": "^9.1.0",
    "postcss-cli": "^11.0.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.288",
    "@iconify/tailwind": "^1.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "netlify-cli": "^17.38.1",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17"
  }
}
